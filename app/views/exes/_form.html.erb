<%= form_for(@ex) do |f| %>

	<p>
		<%= f.label :name %>
		<%= f.text_field :name %>
	</p>
	<p>
		<%= f.label :description %>
		<%= f.text_area :description %>
	</p>
	<p>
		<%= f.radio_button(:category, 'cardio', :id => 'cardio') %> Cardio
		<%= f.radio_button(:category, 'strength', :id => 'strength') %> Strength
		<%= f.radio_button(:category, 'stretch', :id => 'stretch') %> Stretch
	</p>
	<p>
		<%= f.label :url, 'url' %>
		<%= f.text_field :url %>
	</p>
	<p>
		<%= f.submit "Add" %>
	</p>




<% end %>



