<!DOCTYPE html>
<html>
<head>
  <title>Workoutgen</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<%= link_to "Add exercise", new_ex_path %>

	<% flash.each do |key, value| %>
	  <div class='flash' id='<%=key %>'>
  	  <%= value %>
  	</div>
	<% end %>

<%= yield %>

</body>
</html>
